{
	"version": 2,
	"public": true,
	"functionFailoverRegions": ["sin1", "hkg1"],
	"builds": [
		{ 
			"src": "app/main.py", 
			"use": "@vercel/python" 
		}
	],
	"routes": [
		{
			"src": "/",
			"dest": "app/main.py"
		},
		{
			"src": "/health",
			"dest": "app/main.py"
		},
		{
			"src": "/alert-hook",
			"dest": "app/main.py"
		},
		{
			"src": "/docs",
			"dest": "app/main.py"
		}
	]
}